{
    $schema: 'https://docs.renovatebot.com/renovate-schema.json',
    baseBranches: ['main'],
    enabledManagers: ['npm'],
    // Only fetch release notes for breaking changes
    fetchReleaseNotes: false,
    // PRs will be skipped unless a non-pending version
    // is available (applies to "stabilityDays" check only)
    internalChecksFilter: 'strict',
    // Prevent DDos github platform
    branchConcurrentLimit: 3,
    prConcurrentLimit: 3,
    labels: ['renovate'],
    repositories: ['tinkoff/taiga-ui'],
    username: 'renovate',
    gitAuthor: 'Renovate Bot <bot@renovateapp.com>',
    platform: 'github',
    masterIssueApproval: false,
    separateMajorMinor: true,
    separateMinorPatch: false,
    dependencyDashboard: false,
    dependencyDashboardApproval: false,
    // Always bump minor/patch to latest
    rangeStrategy: 'bump',
    branchPrefix: 'renovate/',
    timezone: 'Europe/Moscow',
    // Required to prevent onboarding pr and generation
    // of new config file https://github.com/renovatebot/github-action#configurationfile
    onboarding: false,
    requireConfig: false,
    digest: {
        enabled: false,
    },
    pinDigests: false,
    includePaths: ['package.json', '**/package.json', '!node_modules/**'],
    prBodyTemplate: '{{{table}}}{{{notes}}}{{{changelogs}}}{{{controls}}}{{{footer}}}',
    prBodyColumns: ['Package', 'Type', 'Change'],
    packageRules: [
        // Disable
        {
            enabled: false,
            matchUpdateTypes: ['major'],
        },
        {
            enabled: false,
            depTypeList: ['engines', 'peerDependencies', 'optionalDependencies'],
        },
        {
            enabled: false,
            matchUpdateTypes: ['major', 'minor', 'patch'],
            matchPackageNames: [
                '@nguniversal/common',
                '@nguniversal/express-engine',
                '@angular-builders/custom-webpack',
                '@angular-devkit/schematics',
                '@angular-devkit/build-angular',
                '@angular-devkit/core',
                '@angular/language-service',
                '@angular/animations',
                '@angular/cdk',
                '@angular/common',
                '@angular/compiler',
                '@angular/core',
                '@angular/forms',
                '@angular/cli',
                '@angular/compiler-cli',
                '@angular/localize',
                '@angular/platform-browser',
                '@angular/platform-browser-dynamic',
                '@angular/platform-server',
                '@angular/router',
                'zone.js',
                'rxjs',
            ],
        },
        {
            matchUpdateTypes: ['major', 'minor', 'patch'],
            matchPackageNames: ['typescript', 'tslib'],
            enabled: false,
        },
        // Enable
        {
            matchUpdateTypes: ['minor'],
            labels: ['renovate', 'minor'],
        },
        {
            matchUpdateTypes: ['patch'],
            labels: ['renovate', 'patch'],
        },
        {
            groupName: 'ng-web-apis',
            matchUpdateTypes: ['minor', 'patch'],
            matchPackagePatterns: ['^@ng-web-apis/.*'],
        },
        {
            groupName: 'tiptap',
            matchUpdateTypes: ['minor', 'patch'],
            matchPackagePatterns: ['^@tiptap/.*', '^prosemirror.*'],
        },
        {
            groupName: 'types',
            matchUpdateTypes: ['minor', 'patch'],
            matchPackagePatterns: ['^@types/.*'],
        },
        {
            groupName: 'prettier',
            matchUpdateTypes: ['minor', 'patch'],
            matchPackagePatterns: ['^prettier'],
        },
        {
            groupName: 'nrwl',
            matchUpdateTypes: ['minor', 'patch'],
            matchPackagePatterns: ['^@nrwl/.*'],
        },
        {
            groupName: 'cypress',
            matchUpdateTypes: ['minor', 'patch'],
            matchPackagePatterns: ['cypress', 'cy2', '@types/cypress-image-snapshot', 'cypress-image-snapshot'],
        },
        {
            groupName: 'commitlint',
            matchUpdateTypes: ['minor', 'patch'],
            matchPackagePatterns: ['^@commitlint/.*'],
        },
        {
            groupName: 'tinkoff',
            matchUpdateTypes: ['minor', 'patch'],
            matchPackagePatterns: ['^@tinkoff/.*'],
        },
        {
            groupName: 'icons',
            matchUpdateTypes: ['minor', 'patch'],
            matchPackagePatterns: ['^rollup.*', '@types/svgo', '@rollup/pluginutils', 'svgo'],
        },
        {
            groupName: 'tools',
            matchUpdateTypes: ['minor', 'patch'],
            matchPackagePatterns: [
                'find-in-files',
                'ts-loader',
                'ts-node',
                'wait-on',
                'kill-port',
                'http-server',
                'firebase-tools',
                'husky',
                'postcss',
                'standard-version',
                'angular-http-server',
                'bundlemon',
                'parse5',
                'core-js',
                'date-fns',
                'cpy-cli',
                'ng-yandex-metrika',
                'ngx-markdown',
                'dompurify',
                '^@stackblitz/.*',
                'express',
                'less-plugin-npm-import',
                'showdown',
            ],
        },
        {
            groupName: 'eslint',
            matchUpdateTypes: ['minor', 'patch'],
            matchPackagePatterns: [
                '^eslint.*',
                '^@types/eslint$',
                '^@angular-eslint/.*',
                '^@html-eslint/.*',
                '^@typescript-eslint/.*',
                '^@nrwl/eslint-.*',
                '^@tinkoff/eslint-.*',
            ],
        },
        {
            groupName: 'jest',
            matchUpdateTypes: ['minor', 'patch'],
            matchPackagePatterns: ['^@types/jest$', '^@types/jsdom$', '^jest.*', '^ts-jest$', '^jest-preset-angular$'],
        },
        {
            groupName: 'jasmine',
            matchUpdateTypes: ['minor', 'patch'],
            matchPackagePatterns: ['^jasmine.*'],
        },
        {
            groupName: 'renovate',
            matchDatasources: ['github-tags'],
            matchUpdateTypes: ['minor', 'patch'],
            matchPackageNames: ['renovatebot/github-action'],
        },
    ],
}
