            "@taiga-ui/addon-doc",
            "@taiga-ui/addon-charts",
            "@taiga-ui/addon-commerce",
            "@taiga-ui/icons",
            "@taiga-ui/addon-table",
            "@taiga-ui/addon-mobile",
            "@taiga-ui/layout",
            "@taiga-ui/experimental",
            "@taiga-ui/proprietary",
            "@taiga-ui/legacy"
        ]
