{
    "name": "@taiga-ui/cdk",
    "version": "4.52.0",
    "description": "Base library for creating Angular components and applications using Taiga UI principles regarding of actual visual appearance",
    "keywords": [
        "angular",
        "cdk",
        "component",
        "development",
        "kit",
        "service",
        "directive"
    ],
    "homepage": "https://github.com/taiga-family/taiga-ui",
    "repository": "https://github.com/taiga-family/taiga-ui",
    "license": "Apache-2.0",
    "exports": {
        "./schematics/*": "./schematics/*"
    },
    "dependencies": {
        "tslib": "2.8.1"
    },
    "devDependencies": {
        "@angular-devkit/core": "19.2.19",
        "@angular-devkit/schematics": "19.2.19",
        "@angular-devkit/schematics-cli": "19.2.19",
        "@schematics/angular": "19.2.19"
    },
    "peerDependencies": {
        "@angular/animations": ">=19.0.0",
        "@angular/cdk": ">=19.0.0",
        "@angular/common": ">=19.0.0",
        "@angular/core": ">=19.0.0",
        "@angular/forms": ">=19.0.0",
        "@ng-web-apis/common": "^4.12.2",
        "@ng-web-apis/mutation-observer": "^4.12.2",
        "@ng-web-apis/platform": "^4.12.1",
        "@ng-web-apis/resize-observer": "^4.12.2",
        "@ng-web-apis/screen-orientation": "^4.12.1",
        "@taiga-ui/event-plugins": "^4.7.0",
        "@taiga-ui/polymorpheus": "^4.9.0",
        "rxjs": ">=7.0.0"
    },
    "optionalDependencies": {
        "@angular-devkit/core": ">=19.0.0",
        "@angular-devkit/schematics": ">=19.0.0",
        "@schematics/angular": ">=19.0.0",
        "ng-morph": "^4.8.4",
        "parse5": "^8.0.0"
    },
    "ng-update": {
        "migrations": "./schematics/migration.json",
